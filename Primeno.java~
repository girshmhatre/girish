import java.util.*;
class Primeno
{
public static void main(String arg[])
{
int no,i;
Scanner sc= new Scanner(System.in);
System.out.println("enter the number");
no=sc.nextInt();
for(i=2;i<no;i++)
{
   if(no%i==0)
  {
    System.out.println("not  prime no");
  }
   else
    {
   System.out.println("prime");
     }
  }
 }
}
